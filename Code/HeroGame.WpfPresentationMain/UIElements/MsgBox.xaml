<Window x:Class="HeroGame.WpfPresentationMain.UIElements.MsgBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MsgBox" Height="170" Width="320" 
        MaxHeight="170" MaxWidth="320" MinHeight="170" MinWidth="320"
        Background="#FF32B4B4" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock Name="Message" Text="Зареєстровані" Margin="10,39,10,60" TextAlignment="Center" TextWrapping="Wrap" FontSize="18" FontFamily="Buxton Sketch"/>
        <Button Content="Ok" Margin="0,90,0,0" Height="35" Width="120" Background="Orange" BorderBrush="Black" Click="Button_Click">
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>
    </Grid>
</Window>
